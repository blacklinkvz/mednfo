  <div class="col" id="<%= dom_id travel %>">
        <div class="card mb-4 rounded-3 shadow-sm">
          <div class="card-header py-3">
            <h4 class="my-0 fw-normal"><%= travel.name %></h4>
          </div>
          <div class="card-body">
            <ul class="list-unstyled mt-3 mb-4">
              <li>Fecha salida: <%= travel.start_date %></li>
              <li>Fecha regreso: <%= travel.finish_date %></li>
            </ul>
            <%= link_to "Ver detalle", travel, :class => "w-100 btn btn-lg btn-primary rounded-pill px-2" %>
          </div>
        </div>
      </div>
      